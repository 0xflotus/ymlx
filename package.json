{
  "name": "ymlx",
  "version": "1.0.1-pre.0",
  "description": "Command-line YAML processing tool",
  "repository": "https://github.com/matthewadams/ymlx",
  "author": "Matthew Adams <ymlx@matthewadams.me>",
  "license": "MIT",
  "bin": {
    "ymlx": "index.js"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "u": "mocha test/*test.js",
    "test": "nyc --reporter=html mocha test/*test.js",
    "posttest": "npm run lint && nyc report",
    "format": "standard --fix",
    "lint": "standard --verbose"
  },
  "keywords": [
    "yaml",
    "yml",
    "cli"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "get-stdin": "6.0.0",
    "js-yaml": "3.12.0",
    "meow": "5.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "dirty-chai": "2.0.1",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "pkg": "4.3.3",
    "release-it": "7.2.1",
    "standard": "11.0.1"
  }
}
